{
  "name": "@vue-auto-i18n/cli",
  "version": "2.0.0",
  "description": "CLI tool for Vue Auto I18n",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "vue-auto-i18n": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@vue-auto-i18n/core": "^2.0.0",
    "@vue-auto-i18n/replacer": "^2.0.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "dotenv": "^16.6.1",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.3",
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "vue",
    "i18n",
    "cli",
    "command-line"
  ],
  "author": "",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
