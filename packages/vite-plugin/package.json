{
  "name": "@vue-auto-i18n/vite-plugin",
  "version": "2.0.0",
  "description": "Vite plugin for Vue Auto I18n - automatic AST transformation",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@vue-auto-i18n/core": "^2.0.0",
    "@vue/compiler-sfc": "^3.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@vue-auto-i18n/core": "^2.0.0",
    "vite": "^4.0.0 || ^5.0.0"
  },
  "keywords": [
    "vite",
    "plugin",
    "vue",
    "i18n",
    "ast",
    "transformation"
  ],
  "author": "",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
